var ls_w = 25, ls_h = 25;

legend.append("rect")
	.attr("x", 50)
	.attr("y", function(d, i){return height - (i*ls_h) - 2*ls_h;})
	<!-- .attr('transform', 'translate(10, -720)') /* XY position of legend*/ -->
	.attr('transform', 'translate(220, -555)') /* XY position of legend*/
	.attr("width", ls_w)
	.attr("height", ls_h)
	.style("fill", function(d, i){return color(d);})
