function getDMcolor(d) {
	return d > 8 ? '#6E3936' :
		   d > 7  ? '#875A50' :
		   d > 6  ? '#A3816F' :
		   d > 5  ? '#BFAD91' :
		   d > 4  ? '#DBDBB6' :
		   d > 3   ? '#B9C4B9' :
		   d > 2   ? '#97ACBA' :
		   d > 1   ? '#7799BD' :
					  '#5183BD'; 
}

function updateClasses() {
	svg.selectAll(".collection path")
		.attr("fill", function (d){return getDMcolor(d.id); })
		//.attr("opacity", 0.9)
}



